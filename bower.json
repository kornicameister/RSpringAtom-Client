{
   "name"           : "SpringAtom",
   "version"        : "0.0.2-alpha",
   "homepage"       : "https://github.com/kornicameister/SpringAtom",
   "repository"     : {
      "type": "git",
      "url" : "git://github.com/kornicameister/SpringAtom.git"
   },
   "description"    : "SpringAtom - Client Side Package",
   "main"           : "static/sa/springatom.js",
   "keywords"       : [
      "spring",
      "car",
      "service",
      "management"
   ],
   "authors"        : [
      "kornicameister@gmail.com"
   ],
   "license"        : "MIT",
   "private"        : true,
   "ignore"         : [
      "**/.*",
      "node_modules",
      "bower_components",
      "vendor",
      "build"
   ],
   "overrides"      : {
      "angular"          : {
         "dev" : "angular.js",
         "prod": "angular.min.js"
      },
      "angular-animate"  : {
         "dev" : "angular-animate.js",
         "prod": "angular-animate.min.js"
      },
      "angular-bootstrap": {
         "dev" : "ui-bootstrap-tpls.js",
         "prod": "ui-bootstrap-tpls.min.js"
      },
      "angular-growl-v2" : {
         "dev" : [
            "angular-growl.js",
            "angular-growl.css"
         ],
         "prod": [
            "angular-growl.min.js",
            "angular-growl.min.css"
         ]
      }
   },
   "dependencies"   : {
      "jsface"                       : "latest",
      "lodash"                       : "3.3.0",
      "animate.css"                  : "3.2.x",
      "normalize-css"                : "3.0.x",
      "angular"                      : "1.4.x",
      "angular-bootstrap"            : "0.12.x",
      "angular-ui-router"            : "0.2.x",
      "angular-ui-grid"              : "3.0.x",
      "angular-ui-calendar"          : "0.9.x",
      "angular-animate"              : "1.4.x",
      "angular-moment"               : "0.9.x",
      "angular-cookies"              : "1.4.x",
      "angular-dialog-service"       : "5.2.x",
      "angular-translate"            : "2.6.x",
      "angular-translate-handler-log": "2.6.x",
      "angular-local-storage"        : "0.1.x",
      "angular-growl-v2"             : "0.7.x",
      "ui-router-extras"             : "~0.0.13",
      "bootstrap"                    : "3.3.x",
      "jquery"                       : "latest",
      "font-awesome"                 : "~4.3.0",
      "bowser"                       : "~0.7.2",
      "restangular"                  : "1.4.x"
   },
   "devDependencies": {
      "angular-mocks"   : "latest",
      "angular-scenario": "latest",
      "angular-growl-v2": "~0.7.3"
   },
   "resolutions"    : {
      "angular-bootstrap": "0.12.x",
      "angular-translate": "2.6.x",
      "angular"          : "1.4.x",
      "lodash"           : "3.3.0"
   }
}
